	<!-- 辖区所有居民 -->
	<select id="jmxxlistforApp" parameterType="pd" resultType="pd">
			SELECT *
				  FROM ( SELECT ROWNUM AS rowno,
			USER_AGENT_ID, USER_NAME,TELEPHONE,EMAIL,NICK_NAME,SEX,ID_NUMBER,MI_CARD,IS_SIGN,ADDRESS
		FROM  
			<include refid="tableName"></include> 
		<where> 
			<if test="USER_NAME != null and USER_NAME != ''"> <!-- 姓名检索  -->
				 USER_NAME LIKE CONCAT(CONCAT('%',#{USER_NAME}),'%')
			</if>
			<if test="TELEPHONE != null and TELEPHONE != ''"> <!-- 手机号检索  -->
			  and
				 TELEPHONE LIKE CONCAT(CONCAT('%',#{TELEPHONE}),'%')
			</if>
			<if test="ID_NUMBER != null and ID_NUMBER != ''"> <!-- 身份证号检索  -->
		      and
				 ID_NUMBER LIKE CONCAT(CONCAT('%',#{ID_NUMBER}),'%')
			</if>
			 and STATE='0'
		</where>
		ORDER BY USER_CREATE_TIME DESC  ) table_alias
				 WHERE table_alias.rowno BETWEEN #{pageStart} and #{pageEnd}
	</select>
