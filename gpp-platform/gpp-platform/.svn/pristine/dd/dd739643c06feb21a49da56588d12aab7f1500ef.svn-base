                e.hdsb0402010101_001,
                e.hdsb0402010101_002,
                e.hdsb0402010101_003,
                e.hdsb0402010101_004,
                e.hdsb0402010101_005,
                g.guid as hdsb0402010103_ID ,
                g.hdsb0402010103_001,
                g.hdsb0402010103_002,
                g.hdsb0402010103_003,
                f.*
           FROM HDSB04020101 a
           LEFT JOIN HDSB040201 b
             ON a.GUID = b.guid
           LEFT JOIN HDSB0402 c
             ON c.GUID = b.hdsb040201_001
           LEFT JOIN HDSA0001 d
             ON d.GUID = c.hdsb0402_001
           LEFT JOIN HDSB0402010101 e
             ON e.HDSB0402010101_001 = c.guid
           LEFT JOIN HDSB0402010102 f
             ON f.HDSB0402010102_001 = c.guid
           LEFT JOIN HDSB0402010103 g
             ON g.HDSB0402010103_001 = c.guid
          WHERE
           d.HDSA0001_003 = '42010519360522121X'
            and b.HDSB040201_003 >= to_date('2011-06-08', 'yyyy-mm-dd')
            and b.HDSB040201_003 <= to_date('2017-06-08', 'yyyy-mm-dd')
