package com.tbyf.util.jpush.entity;

import java.io.Serializable;
/**
 * 推送消息实体类
 * @author zanxc
 * @创建日期2017年8月11日下午3:13:35
 */
public class PushDevice implements Serializable {

	 private static final long serialVersionUID = 1L;
	 
	 public String userId ;//用户id
	 public String userType;//用户类型
	 public String deviceId;//设备的推送ID
	 public String deviceType;//设备类型
	 public String deviceName;//设备名称
	 public Integer isOpenbroad;//是否开启广播
	 public Integer isOpenStopMessage;//是否开启站内信
	 public Integer isOpenConsultMessage;//是否开启咨询消息
	 public Integer isOpenJobMessage;//是否开启任务提醒
	 public PushDevice() {
		 
	 }
	 public PushDevice(String userId, String userType, String deviceId, String deviceType) {
		 this.deviceId = deviceId;
		 this.deviceType = deviceType;
		 this.userId = userId;
		 this.userType = userType;
	 } 
     public PushDevice( String deviceId, String deviceType) {
    	 this.deviceId = deviceId;
		 this.deviceType = deviceType;
	 }
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public String getUserType() {
		return userType;
	}
	public void setUserType(String userType) {
		this.userType = userType;
	}
	public String getDeviceId() {
		return deviceId;
	}
	public void setDeviceId(String deviceId) {
		this.deviceId = deviceId;
	}
	public String getDeviceType() {
		return deviceType;
	}
	public void setDeviceType(String deviceType) {
		this.deviceType = deviceType;
	}
	public String getDeviceName() {
		return deviceName;
	}
	public void setDeviceName(String deviceName) {
		this.deviceName = deviceName;
	}
	public Integer getIsOpenbroad() {
		return isOpenbroad;
	}
	public void setIsOpenbroad(Integer isOpenbroad) {
		this.isOpenbroad = isOpenbroad;
	}
	public Integer getIsOpenStopMessage() {
		return isOpenStopMessage;
	}
	public void setIsOpenStopMessage(Integer isOpenStopMessage) {
		this.isOpenStopMessage = isOpenStopMessage;
	}
	public Integer getIsOpenConsultMessage() {
		return isOpenConsultMessage;
	}
	public void setIsOpenConsultMessage(Integer isOpenConsultMessage) {
		this.isOpenConsultMessage = isOpenConsultMessage;
	}
	public Integer getIsOpenJobMessage() {
		return isOpenJobMessage;
	}
	public void setIsOpenJobMessage(Integer isOpenJobMessage) {
		this.isOpenJobMessage = isOpenJobMessage;
	}
	
     
}
